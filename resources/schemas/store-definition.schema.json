{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
        "storePresetName": {
            "type": "string"
        },
        "locales": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "currencies": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "countries": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "menuTaxon": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "translations": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            },
            "required": [
                "code",
                "name",
                "translations"
            ],
            "additionalProperties": false
        },
        "channel": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "locales": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "currencies": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "hostname": {
                    "type": "string"
                },
                "theme_name": {
                    "type": "string"
                }
            },
            "required": [
                "code",
                "name",
                "locales",
                "currencies",
                "hostname",
                "theme_name"
            ],
            "additionalProperties": false
        },
        "categories": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "code": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    },
                    "slug": {
                        "type": "string"
                    },
                    "translations": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    }
                },
                "required": [
                    "code",
                    "name",
                    "slug",
                    "translations"
                ],
                "additionalProperties": false
            }
        },
        "paymentMethods": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "code": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    }
                },
                "required": [
                    "code",
                    "name"
                ],
                "additionalProperties": false
            }
        },
        "shippingMethods": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "code": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    }
                },
                "required": [
                    "code",
                    "name"
                ],
                "additionalProperties": false
            }
        },
        "taxCategories": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "code": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    }
                },
                "required": [
                    "code",
                    "name"
                ],
                "additionalProperties": false
            }
        },
        "taxons": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "code": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    },
                    "translations": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    }
                },
                "required": [
                    "code",
                    "name",
                    "translations"
                ],
                "additionalProperties": false
            }
        },
        "taxRates": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "code": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    },
                    "zone": {
                        "type": "string"
                    },
                    "category": {
                        "type": "string"
                    },
                    "amount": {
                        "type": "number"
                    }
                },
                "required": [
                    "code",
                    "name",
                    "zone",
                    "category",
                    "amount"
                ],
                "additionalProperties": false
            }
        },
        "products": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "description": {
                        "type": "string"
                    },
                    "short_description": {
                        "type": "string"
                    },
                    "img_prompt": {
                        "type": "string"
                    },
                    "tax_category": {
                        "type": "string"
                    },
                    "main_taxon": {
                        "type": "string"
                    },
                    "taxons": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "product_attributes": {
                        "type": "object",
                        "additionalProperties": {
                            "type": "string"
                        }
                    },
                    "product_options": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "images": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    }
                },
                "required": [
                    "name",
                    "description",
                    "short_description",
                    "img_prompt",
                    "tax_category",
                    "main_taxon",
                    "taxons",
                    "product_attributes",
                    "images"
                ],
                "additionalProperties": false
            }
        },
        "theme": {
            "type": "object",
            "description": "Wygenerowany theme sklepu (cssVariables, assets)",
            "properties": {
                "cssVariables": {
                    "type": "object",
                    "patternProperties": {
                        "^--[a-zA-Z0-9-]+$": {
                            "type": "string"
                        }
                    }
                },
                "assets": {
                    "type": "object",
                    "properties": {
                        "logo": {
                            "type": "string"
                        },
                        "banner": {
                            "type": "string"
                        }
                    },
                    "additionalProperties": {
                        "type": "string"
                    }
                }
            },
            "required": [
                "cssVariables",
                "assets"
            ]
        }
    },
    "required": [
        "storePresetName",
        "locales",
        "currencies",
        "countries",
        "menuTaxon",
        "channel",
        "categories",
        "paymentMethods",
        "shippingMethods",
        "taxCategories",
        "taxons",
        "taxRates",
        "products",
        "theme"
    ],
    "additionalProperties": false
}
